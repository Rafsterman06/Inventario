<!doctype html>
<html lang="es">
    <head>
        <meta charset="utf-8">
        <title>La Ceiba</title>    
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="author" content="Videojuegos & Desarrollo">
        <meta name="description" content="Ejemplo de formulario de acceso basado en HTML5 y CSS">
        <meta name="keywords" content="login,formulariode acceso html">
        <link rel="shortcut icon" href="/sources/img/icons8-oak-tree-16.png" type="image/x-icon">
        <link href="https://fonts.googleapis.com/css?family=Nunito&display=swap" rel="stylesheet"> 
        <link rel="stylesheet" href="../sources/css/cliente.css">
    </head>
    <body>
        <div id="contenedor">
            <div id="contenedortabla">
                <a href="/goformcreatedetallesalida"><img src="./sources/img/plus.png" alt="createcliente"></a>
                <table>
                    <tr>
                        <th>ID DETALLE SALIDA</th>
                        <th>ID SALIDA</th>
                        <th>ID PRODUCTO</th>
                        <th>FECHA</th>
                        <th>CANTIDAD</th>
                    </tr>
                    <tbody id="tabladetallesalida">
                        <% if (dato.length) { %>
                            <% for( let index = 0; index < dato.length; index++ ) { %>
                                <tr><td><%=dato[index].id_detallesalida%></td><td><%=dato[index].id_salida%></td><td><%= dato[index].id_product %></td><td><%=dato[index].fecha%></td><td><%=dato[index].cantidad%></td><td><a href="/goformupdatedetallesalida/<%=dato[index].id_detallesalida%>"><img src="./sources/img/edit.png" alt="update"></a></td><td><a href="/deletedetallesalida/<%=dato[index].id_detallesalida%>"><img src="./sources/img/cross.png" alt="delete"></a></td></tr>
                            <% } %>
                        <% } %>
                    </tbody>
                </table>
            </div>
        </div>
    </body>
</html>